{
	"type": "modal",
	"callback_id": "get_test_status_submit",
	"title": {
		"type": "plain_text",
		"text": "Stress test status"
	},
	"close": {
		"type": "plain_text",
		"text": "Cancel",
		"emoji": true
	},
	"submit": {
		"type": "plain_text",
		"text": "Get",
		"emoji": true
	},
	"blocks": [
		{
			"type": "header",
			"text": {
				"type": "plain_text",
				"text": "Get stress test status",
				"emoji": true
			}
		},
		{
			"type": "input",
			"block_id": "{{ .NameBlock }}",
			"label": {
				"type": "plain_text",
				"text": "Test name",
				"emoji": true
			},
			"element": {
				"type": "plain_text_input",
				"multiline": false,
				"action_id": "{{ .NameAction }}",
				"placeholder": {
					"type": "plain_text",
					"text": " ",
					"emoji": true
				}
			}
		}
	]
}